{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
